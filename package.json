    "@adonisjs/repl": "1.1.6",
    "@adonisjs/session": "^4.0.6",
    "@adonisjs/view": "^3.1.0",
    "adonis5-sentry": "^1.1.0",
    "adonis5-swagger": "^1.2.3",
    "axios": "^0.21.1",
    "luxon": "^1.26.0",
    "mysql": "^2.18.1",
    "phc-argon2": "^1.1.0",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3",
    "ts-to-openapi": "^1.3.0"
  }
}
